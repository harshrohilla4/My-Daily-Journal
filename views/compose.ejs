<%-include("partials/header")-%>
<h1>Compose</h1>
<form  action="/compose" method="post">
    <div class="form-group">
        <label for="posttitle">Title</label>
        <input class="form-control" type="text" name="posttitle" id="posttitle"/>
        <label for="postbody">Post</label>
        <textarea class="form-control" name="postbody" id="postbody" rows="5"cols="30"></textarea>
    </div>
    <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>

<%-include("partials/footer") -%>
